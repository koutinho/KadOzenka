{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "Proxy.Program": "Verbose"
      }
    },
    "Enrich": [
      "WithMachineName",
      "WithEnvironmentUserName",
      "WithMemoryUsage",
      "WithThreadId",
      "WithProcessId",
      "FromLogContext",
      "HttpContextData"
    ],
    "Properties": {
      "ApplicationName": "YandexAPI_Proxy"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://10.80.214.38:8081"
        }
      }
    ]
  }
}