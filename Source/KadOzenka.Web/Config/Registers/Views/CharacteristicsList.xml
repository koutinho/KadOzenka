    <Button Caption="Удалить" NavigateURL="/ObjectsCharacteristics/DeleteCharacteristic" WindowHeight="180" WindowWidth="840" NeedRefresh="true">
      <Parameters>
        <Parameter Key="attributeId" Value="[ObjectID]" />
