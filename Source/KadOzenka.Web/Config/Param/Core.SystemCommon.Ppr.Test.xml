<?xml version="1.0" encoding="utf-8"?>
<SystemCommon xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <ApplicationName>Тест (TEST) - Кадастровая оценка</ApplicationName>
    <ApplicationSubName>Центр имущественных платежей и жилищного страхования</ApplicationSubName>
    <LeftPanelTitle>Департамент имущества города Москвы</LeftPanelTitle>
    <LeftPanelDescription>Центр имущественных платежей и жилищного страхования</LeftPanelDescription>
    <KendoTheme>silver.genix</KendoTheme>
    <DefaultLayout>_LayoutLeftPanel</DefaultLayout>
    <DesignType>LeftPanel</DesignType>
    <ShowHelpSection>true</ShowHelpSection>

    <ActiveDirectoryDomain>cipgs.ru</ActiveDirectoryDomain>

    <DefaultDomainName>CIPGS</DefaultDomainName>

    <SrdSettings>
      <UseRoleSubsystem>false</UseRoleSubsystem>
    </SrdSettings>

    <MessageSettings>
      <!--Частота проверки наличия новых личных сообщений (в секундах). 0 - проверка отключена.-->
      <Delay>5</Delay>
    </MessageSettings>
  
    <MailServer>
      <Host>pochta.mos.ru</Host>
      <Port>25</Port>
      <Login>ops_system@property.mos.ru</Login>
      <Password></Password>
      <EnableSsl>false</EnableSsl>
      <FromEmail>ops_noreply@property.mos.ru</FromEmail>
    </MailServer>
  
    <ErrorManagment>
        <Phone></Phone>
        <Email></Email>
    </ErrorManagment>

  <AttachmnetsSettings>
    <IsFileStorage>true</IsFileStorage>
  </AttachmnetsSettings>

</SystemCommon>