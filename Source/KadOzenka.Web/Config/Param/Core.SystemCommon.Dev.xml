  <MailServer>
    <Host>pochta.mos.ru</Host>
    <Port>25</Port>
    <Login>ops_system@property.mos.ru</Login>
    <Password></Password>
    <EnableSsl>false</EnableSsl>
    <FromEmail>ops_noreply@property.mos.ru</FromEmail>
  </MailServer>

  <ErrorManagment>
    <Phone></Phone>
    <Email></Email>
  </ErrorManagment>

  <OmAssemblies>
    <string>Platform</string>
    <string>KadOzenka.ObjectModel</string>
  </OmAssemblies>
