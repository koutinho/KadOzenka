@using KadOzenka.Web.Helpers
@model List<KadOzenka.Web.Models.GbuObject.PartialAttribute>
@{
    var startRowNumber = (int) ViewData["StartRowNumber"];
    var rowCount = (int)ViewData["RowCount"];
}
@for (int rowNumber = startRowNumber, i = 0; rowNumber < startRowNumber + rowCount; rowNumber++, i++)
{
    <div class="form-group">
        <div class="col-sm-10 col-sm-offset-2">
            <div>
                @Html.KendoDropDownListTreeWithButton(m => m[i].Attributes, (IEnumerable<DropDownTreeItemModel>)ViewData["TreeAttributes"], idPrefix: "_" + rowNumber)
            </div>
        </div>
    </div>
}
