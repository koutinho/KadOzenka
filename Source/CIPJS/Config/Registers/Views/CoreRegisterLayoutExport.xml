<?xml version="1.0" encoding="utf-8"?>
<RegisterView IsBasic="true" RegisterId="956" DefaultLayoutId="1000956" Description="Менеджер загрузки" LinkedRegistersList="956,933,930,950" 
              ShowActualDate="false" ShowFilterMenu="false" ShowSettingsPanel="false" UseMasterPage="false" 
              SearchPanel="CoreRegisterLayoutExport" DockNavigationPanel="true" SRDFunctionTag="CORE.REGISTER.EXPORT">
    <Buttons>
        <Button Caption="Запустить формирование новой выгрузки" Type="DropDown">
            <DropDownButtons>
                <Button Caption="Xlsx (Excel 2007 и др)" NavigateURL="/CoreRegisterLayout/ExportStart" NeedRefresh="true" WindowHeight="200" WindowWidth="400">
                    <Parameters>
                      <Parameter Key="layoutId" Value="{Get:ExportLayoutId}" />
                      <Parameter Key="type" Value="Xlsx" />
                      <Parameter Key="registerViewId" Value="{Get:ExportRegisterViewId}" />
                    </Parameters>
                </Button>
                <Button Caption="Xls (Excel 97 - 2003)" NavigateURL="/CoreRegisterLayout/ExportStart" NeedRefresh="true" WindowHeight="200" WindowWidth="400">
                    <Parameters>
                       <Parameter Key="layoutId" Value="{Get:ExportLayoutId}" />
                       <Parameter Key="type" Value="Xls" />
                      <Parameter Key="registerViewId" Value="{Get:ExportRegisterViewId}" />
                    </Parameters>
                </Button>
                <Button Caption="Csv" NavigateURL="/CoreRegisterLayout/ExportStart" NeedRefresh="true" WindowHeight="200" WindowWidth="400">
                    <Parameters>
                      <Parameter Key="layoutId" Value="{Get:ExportLayoutId}" />
                      <Parameter Key="type" Value="Csv" />
                      <Parameter Key="registerViewId" Value="{Get:ExportRegisterViewId}" />
                    </Parameters>
                </Button>
                <Button Caption="Dbf" NavigateURL="/CoreRegisterLayout/ExportStart" NeedRefresh="true" WindowHeight="200" WindowWidth="400">
                    <Parameters>
                      <Parameter Key="layoutId" Value="{Get:ExportLayoutId}" />
                      <Parameter Key="type" Value="Dbf" />
                      <Parameter Key="registerViewId" Value="{Get:ExportRegisterViewId}" />
                    </Parameters>
                </Button>
            </DropDownButtons>
        </Button>
        <Button Caption="Скачать результат" NavigateURL="/CoreRegisterLayout/ExportDownload" OpenInNewWindow="true" SRDFunctionTag="CORE.REGISTER.EXPORT">
            <Parameters>
                <Parameter Key="exportId" Value="[ObjectID]" />
            </Parameters>
            <ConditionGroups>
                <ConditionGroup HideOrDisable="Disable">
                    <Conditions>
                        <Condition Key="[95600600]" Operation="NotEqual" Value="2" />
                    </Conditions>
                </ConditionGroup>
            </ConditionGroups>
        </Button>
        <Button Caption="Дополнительно" Type="DropDown">
            <DropDownButtons>
                <Button Caption="Повторить формирование результата" NavigateURL="/CoreRegister/ExportReStart" NeedRefresh="true" WindowHeight="200" WindowWidth="400">
                    <Parameters>
                        <Parameter Key="exportId" Value="[ObjectID]" />
                    </Parameters>
                    <ConditionGroups>
                        <ConditionGroup HideOrDisable="Disable">
                            <Conditions>
                                <Condition Key="[95600600]" Operation="NotEqual" Value="2" />
                                <Condition Key="[95600600]" Operation="NotEqual" Value="3" AndOr="And" />
                            </Conditions>
                        </ConditionGroup>
                    </ConditionGroups>
                </Button>
                <Button Caption="Открыть SQL&amp;#8209;запрос" NavigateURL="~/CoreUi/ViewQuery" SRDFunctionTag="CORE.REGISTER.LAYOUTADVANCED" WindowHeight="400" WindowWidth="500">
                    <Parameters>
                        <Parameter Key="ExportId" Value="[ObjectID]" />
                        <Parameter Key="Type" Value="SQL" />
                    </Parameters>
                </Button>
                <Button Caption="Открыть XML&amp;#8209;запрос" NavigateURL="~/CoreUi/ViewQuery" SRDFunctionTag="CORE.REGISTER.LAYOUTADVANCED" WindowHeight="400" WindowWidth="500">
                    <Parameters>
                        <Parameter Key="ExportId" Value="[ObjectID]" />
                        <Parameter Key="Type" Value="XML" />
                    </Parameters>
                </Button>
            </DropDownButtons>
            <ConditionGroups>
                <ConditionGroup HideOrDisable="Disable">
                    <Conditions>
                        <Condition Key="NeedSelectedRow" Operation="" Value="true" />
                    </Conditions>
                </ConditionGroup>
            </ConditionGroups>
        </Button>
    </Buttons>
    <Tabs>
        <Tab Caption="Основные данные" NavigateURL="CoreRegisterLayout/LayoutExportInfo">
            <Parameters>
                <Parameter Key="exportId" Value="[ObjectID]" />
            </Parameters>
        </Tab>
    </Tabs>
    <StatusBarConfiguration StatusBarEnabled="false" />
</RegisterView>
