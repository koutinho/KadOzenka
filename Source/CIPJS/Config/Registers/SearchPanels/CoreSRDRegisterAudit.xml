<RootElement>
    <ControlElement Caption="Основные данные" OwnType="RadPanel" Width="0" AttributeNumber="0">
        <children>

            <ControlElement Caption="Дата действия" ID="date_panel" OwnType="Panel">
                <children>
                    <ControlElement Caption="с " ID="date_from" OwnType="Calendar" AttributeNumber="96400300" QueryOperation="MoreThan" />
                    <ControlElement Caption="по" ID="date_to" OwnType="Calendar" AttributeNumber="96400300" QueryOperation="LessThan" />
                </children>
            </ControlElement>

            <ControlElement Caption="Дата действия" ID="date" OwnType="Calendar" AttributeNumber="96400300" QueryOperation="WithinDate" DefaultValue="[CurrentDay]"/>

            <ControlElement ID="changeType" Caption="Тип действия" OwnType="ListBox" AllowItemSelection="true" AttributeNumber="96500600" QueryOperation="Equal" Height="100">
                <CustomItems>
                    <KeyValuePair>
                        <Key>1</Key>
                        <Value>Изменен</Value>
                    </KeyValuePair>
                    <KeyValuePair>
                        <Key>2</Key>
                        <Value>Добавлен</Value>
                    </KeyValuePair>
                    <KeyValuePair>
                        <Key>3</Key>
                        <Value>Удален</Value>
                    </KeyValuePair>
                    <KeyValuePair>
                        <Key>4</Key>
                        <Value>Удален логически</Value>
                    </KeyValuePair>
                </CustomItems>
            </ControlElement>

            <ControlElement ID="changesetType" Caption="Состояние набора изменений" OwnType="ListBox" AllowItemSelection="true" AttributeNumber="96400400" QueryOperation="Equal" Height="80">
                <CustomItems>
                    <KeyValuePair>
                        <Key>0</Key>
                        <Value>Черновик</Value>
                    </KeyValuePair>
                    <KeyValuePair>
                        <Key>1</Key>
                        <Value>Подтверждено</Value>
                    </KeyValuePair>
                    <KeyValuePair>
                        <Key>2</Key>
                        <Value>Отменено</Value>
                    </KeyValuePair>
                </CustomItems>
            </ControlElement>

        </children>
    </ControlElement>

    <ControlElement Caption="Объект" OwnType="RadPanel">
        <children>
            <ControlElement ID="cmbRegister" Caption="Наименование реестра" OwnType="Combo" AttributeNumber="96500300" RegisterId="930" TextFieldAttribute="93000300" ValueFieldAttribute="93000100" QueryOperation="Equal"/>
            <ControlElement ID="registerId" Caption="Номер реестра" OwnType="AutoCompleteBox" MinTypedCharsForSearch="1" AttributeNumber="96500300" ValueFieldAttribute="93000100" QueryOperation="Equal" />
            <ControlElement ID="txLogin" Caption="ИД объекта" OwnType="Text" AttributeNumber="96500400" QueryOperation="Equal" />
        </children>
    </ControlElement>

    <ControlElement Caption="Автор" OwnType="RadPanel">
        <children>
            <ControlElement Caption="Подразделение" ID="cbAuthorDepartment" OwnType="Combo" HasLinkedCombos="true" AttributeNumber="95000200" RegisterId="941" TextFieldAttribute="94100300" ValueFieldAttribute="94100100" QueryOperation="Equal" />
            <ControlElement Caption="ФИО" ID="cbAuthorUser" OwnType="Combo" AttributeNumber="96400500" RegisterId="950" TextFieldAttribute="95000400" ValueFieldAttribute="95000100" ParentFilterAttribute="95000200" QueryOperation="Equal">
                <parentComboBox>
                    <string>cbAuthorDepartment</string>
                </parentComboBox>
            </ControlElement>
        </children>
    </ControlElement>
</RootElement>
